{
  "name": "google-calendar-event-fetcher",
  "version": "0.1.0",
  "description": "Fetch and transform events from the Google Calendar API",
  "keywords": [
    "google",
    "calendar",
    "google-calendar"
  ],
  "homepage": "https://github.com/nstringham/google-calendar-event-fetcher#readme",
  "bugs": {
    "url": "https://github.com/nstringham/google-calendar-event-fetcher/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nstringham/google-calendar-event-fetcher.git"
  },
  "license": "MIT",
  "author": {
    "name": "Nate Stringham",
    "url": "https://github.com/nstringham"
  },
  "type": "module",
  "main": "src/index.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "default": "./src/index.js",
      "types": "./types/index.d.ts"
    },
    "./range-set": {
      "default": "./src/range-set.js",
      "types": "./types/range-set.d.ts"
    },
    "./fullcalendar": {
      "default": "./src/fullcalendar/index.js",
      "types": "./types/fullcalendar/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "test:browser": "vitest run --browser",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint",
    "format": "prettier --write .",
    "prettier": "prettier --check .",
    "version": "version-changelog CHANGELOG.md && git add CHANGELOG.md",
    "prepack": "npm run build"
  },
  "peerDependencies": {
    "@fullcalendar/core": "^6.0.0"
  },
  "peerDependenciesMeta": {
    "@fullcalendar/core": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@fullcalendar/core": "^6.1.20",
    "@vitest/browser-playwright": "^4.0.17",
    "@vitest/coverage-v8": "^4.0.17",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "version-changelog": "^3.1.1",
    "vitest": "^4.0.17"
  }
}
